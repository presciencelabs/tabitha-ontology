{
	"name": "tabitha-ontology",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"prepare": "svelte-kit sync",
		"check": "pnpm prepare && (pnpm check:svelte & pnpm check:lint)",
		"check:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"check:lint": "eslint .",
		"check:lint:fix": "eslint . --fix",
		"build": "vite build",
		"dev": "vite dev",
		"test:e2e": "playwright test",
		"test:e2e:dev": "playwright test --ui",
		"precommit": "pnpm check && pnpm build",
		"clean:svelte": "rimraf .svelte-kit",
		"clean:node": "rimraf node_modules",
		"clean:powerwash": "(pnpm clean:svelte & pnpm clean:node) && pnpm install"
	},
	"dependencies": {
		"@auth/sveltekit": "^1.7.3",
		"@sveltejs/kit": "^2.8.0",
		"@tailwindcss/typography": "^0.5.15",
		"daisyui": "^4.12.14",
		"svelte": "^5.1.12",
		"tailwindcss": "^3.4.14"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20241106.0",
		"@eslint/js": "^9.14.0",
		"@iconify/svelte": "^4.0.2",
		"@playwright/test": "^1.48.2",
		"@stylistic/eslint-plugin": "^2.10.1",
		"@sveltejs/adapter-cloudflare": "^4.7.4",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/node": "^22.9.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.14.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.12.0",
		"postcss": "^8.4.47",
		"rimraf": "^6.0.1",
		"svelte-check": "^4.0.5",
		"svelte-eslint-parser": "^0.43.0",
		"typescript": "^5.6.3",
		"vite": "^5.4.10"
	}
}
