{
	"name": "app",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"prepare": "svelte-kit sync",
		"check": "pnpm prepare && (pnpm check:svelte & pnpm check:lint)",
		"check:svelte": "svelte-check --tsconfig ./jsconfig.json",
		"check:lint": "eslint .",
		"build": "vite build",
		"dev": "wrangler pages dev .svelte-kit/cloudflare",
		"precommit": "pnpm check && pnpm build",
		"clean:svelte": "rimraf .svelte-kit",
		"clean:node": "rimraf node_modules",
		"clean:powerwash": "(pnpm clean:svelte & pnpm clean:node) && pnpm install"
	},
	"dependencies": {
		"@iconify/svelte": "^3.1.6",
		"@sveltejs/kit": "^2.5.0",
		"@tailwindcss/typography": "^0.5.10",
		"daisyui": "^4.6.2",
		"svelte": "^4.2.10",
		"tailwindcss": "^3.4.1"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240129.0",
		"@eslint/js": "^8.56.0",
		"@stylistic/eslint-plugin": "^1.6.0",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.35.1",
		"globals": "^13.24.0",
		"postcss": "^8.4.35",
		"rimraf": "^5.0.5",
		"svelte-check": "^3.6.3",
		"typescript": "^5.3.3",
		"vite": "^5.0.12"
	}
}