{
	"name": "app",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"check": "run-p -l format:check lint kit:check",
		"format:check": "prettier --check .",
		"format:write": "prettier --write .",
		"lint": "eslint .",
		"kit:check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"prepare": "svelte-kit sync",
		"build": "vite build",
		"_build:watch": "vite build -w",
		"_wrangler:dev": "wrangler pages dev .svelte-kit/cloudflare",
		"pages:dev": "PUBLIC_SOURCES_API_HOST=http://localhost:8789 run-p -l _build:watch _wrangler:dev",
		"pages:dev:win": "SET PUBLIC_SOURCES_API_HOST=http://localhost:8789&& pnpm build && pnpm run-p -l _wrangler:dev",
		"pages:list:deployments": "wrangler pages deployment list",
		"clean": "rimraf vite.config.js.timestamp* .svelte-kit test-results",
		"clean:powerwash": "pnpm clean && rimraf node_modules && pnpm install"
	},
	"dependencies": {
		"@iconify/svelte": "^3.1.4",
		"@sveltejs/kit": "^1.27.6",
		"@tailwindcss/typography": "^0.5.10",
		"daisyui": "^4.4.19",
		"svelte": "^4.2.8",
		"tailwindcss": "^3.3.6"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20231121.0",
		"@playwright/test": "^1.40.1",
		"@sveltejs/adapter-cloudflare": "^2.3.3",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.32",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.8",
		"rimraf": "^5.0.5",
		"svelte-check": "^3.6.2",
		"typescript": "^5.3.2",
		"vite": "^5.0.5",
		"vitest": "^0.34.6"
	}
}