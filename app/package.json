{
	"name": "app",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"prepare": "svelte-kit sync",
		"check": "pnpm prepare && (pnpm check:svelte & pnpm check:lint)",
		"check:svelte": "svelte-check --tsconfig ./jsconfig.json",
		"check:lint": "eslint .",
		"check:lint:fix": "eslint . --fix",
		"build": "vite build",
		"dev": "wrangler pages dev .svelte-kit/cloudflare",
		"test:e2e": "playwright test",
		"test:e2e:dev": "playwright test --ui",
		"precommit": "pnpm check && pnpm build",
		"clean:svelte": "rimraf .svelte-kit",
		"clean:node": "rimraf node_modules",
		"clean:powerwash": "(pnpm clean:svelte & pnpm clean:node) && pnpm install"
	},
	"dependencies": {
		"@iconify/svelte": "^3.1.6",
		"@sveltejs/kit": "^2.5.6",
		"@tailwindcss/typography": "^0.5.12",
		"daisyui": "^4.10.2",
		"svelte": "^4.2.15",
		"tailwindcss": "^3.4.3"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240405.0",
		"@eslint/js": "^8.57.0",
		"@playwright/test": "^1.43.1",
		"@stylistic/eslint-plugin": "^1.7.2",
		"@sveltejs/adapter-cloudflare": "^4.4.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-plugin-svelte": "^2.37.0",
		"globals": "^15.0.0",
		"postcss": "^8.4.38",
		"rimraf": "^5.0.5",
		"svelte-check": "^3.6.9",
		"svelte-eslint-parser": "^0.33.1",
		"typescript": "^5.4.5",
		"vite": "^5.2.9"
	}
}